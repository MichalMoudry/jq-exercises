.input.color |
    if . == "black" then 0
    elif . == "brown" then 1
    elif . == "red" then 2
    elif . == "orange" then 3
    elif . == "yellow" then 4
    elif . == "green" then 5
    elif . == "blue" then 6
    elif . == "violet" then 7
    elif . == "grey" then 8
    elif . == "white" then 9
    else
        ["black", "brown", "red", "orange", "yellow", "green", "blue", "violet", "grey", "white"]
    end